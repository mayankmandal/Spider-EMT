<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Spider_EMT</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Spider_EMT.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/css/toastr.min.css" />
</head>
<body>
   
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/bootstrap-datepicker.min.js" ></script>
    <script src="~/js/chart/highcharts.js"></script>
    <script src="~/js/chart/exporting.js"></script>
    <script src="~/js/chart/drilldown.js"></script>
    <script src="~/js/chart/highcharts-3d.js"></script>
    <script src="~/js/chart/highcharts-more.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>